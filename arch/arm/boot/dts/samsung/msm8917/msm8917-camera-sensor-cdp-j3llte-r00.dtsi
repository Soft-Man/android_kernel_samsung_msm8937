	led_flash0: qcom,camera-led-flash {
		cell-index = <0>;
		compatible = "qcom,camera-led-flash";
		qcom,flash-type = <1>;
		qcom,led1-gpio = <&tlmm 33 0x00>;
		qcom,led2-gpio = <&tlmm 34 0x00>;
	};
