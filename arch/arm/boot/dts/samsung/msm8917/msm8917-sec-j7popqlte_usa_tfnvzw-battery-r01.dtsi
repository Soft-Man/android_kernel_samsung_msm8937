		battery,fast_charging_current = <1900 0 500 1900 500 1200 1000 500 0 1200 1000 1900 1900 0 1300 300 700 1300 1500 300 80 0 3250 1000 3250 1400 1200 1000 1000 0 0 1000 1000 1000 1000 1000 1000 1000>;
		battery,full_check_current_1st = <320 0 320 320 320 320 320 320 0 320 320 320 320 0 320 320 320 320 320 320 320 0 320 320 320 320 320 320 320 0 0 320 320 320 320 320 320 320>;
		battery,full_check_current_2nd = <150 0 150 150 150 150 150 150 0 150 150 150 150 0 150 150 150 150 150 150 150 0 150 150 150 150 150 150 150 0 0 150 150 150 150 150 150 150>;

