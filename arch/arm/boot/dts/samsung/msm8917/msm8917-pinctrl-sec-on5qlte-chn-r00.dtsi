					output-high;
				};
			};

			wsa_reset_off: wsa_reset_off {
				mux {
					/*pins = "gpio96";*/
					function = "gpio";
				};

				config {
					/*pins = "gpio96";*/
					drive-strength = <2>; /* 2 MA */
					output-low;
				};
			};
		};

		sdhc2_cd_pin {
			sdc2_cd_on: cd_on {
				mux {
					pins = "gpio67";
					function = "gpio";
				};

				config {
					pins = "gpio67";
