		};

		hall_int {
			hall_int_default: default {
				mux {
					pins = "gpio41";
					function = "gpio";			
				};

				config {
					pins = "gpio41";
					bias-disable;
					drive-strength = <2>;
				};
        		};

			hall_int_sleep: sleep {
				mux {
					pins = "gpio41";
					function = "gpio";			
				};

				config {
					pins = "gpio41";
					bias-disable;
					drive-strength = <2>;
				};
        		};	
		};

		svc_leds_i2c {
			svc_leds_i2c_active: active {
				mux {
					pins = "gpio12" , "gpio13"; /* SDA, SCL */
					function = "NA";			
				};
	
				config {
					pins = "gpio12", "gpio13";
					bias-disable;
					drive-strength = <2>;
				};
			};

			svc_leds_i2c_suspend: suspend {
				mux {
					pins = "gpio12" , "gpio13"; /* SDA, SCL */
					function = "NA";			
				};
	
				config {
					pins = "gpio12", "gpio13";
					bias-disable;
					drive-strength = <2>;
				};
			};
		};
	};
};
