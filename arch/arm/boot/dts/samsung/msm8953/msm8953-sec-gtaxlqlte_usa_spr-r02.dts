		dma-names = "tx", "rx";
	};

/* for camera1*/
	i2c_4: i2c@78b8000 { /* BLSP1 QUP4 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg = <0x78b8000 0x600>;
		interrupt-names = "qup_irq";
		interrupts = <0 98 0>;
		qcom,clk-freq-out = <400000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp1_ahb_clk>,
			<&clock_gcc clk_gcc_blsp1_qup4_i2c_apps_clk>;
		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_vt_cam_active>;
		pinctrl-1 = <&i2c_vt_cam_sleep>;
		qcom,noise-rjct-scl = <0>;
		qcom,noise-rjct-sda = <0>;
		qcom,master-id = <86>;
		dmas = <&dma_blsp1 14 64 0x20000020 0x20>,
			<&dma_blsp1 15 32 0x20000020 0x20>;
		dma-names = "tx", "rx";

         	qcom,camera@1 {
         		cell-index = <1>;
         		compatible = "qcom,camera";
         		reg = <0x1>;
         		qcom,csiphy-sd-index = <1>;
         		qcom,csid-sd-index = <1>;
         		qcom,mount-angle = <270>;
         		cam_vt_vana-supply = <&pm8953_l22>;
         		qcom,cam-vreg-name = "cam_vt_vana";
         		qcom,cam-vreg-min-voltage = <2800000>;
         		qcom,cam-vreg-max-voltage = <2800000>;
         		qcom,cam-vreg-op-mode = <2800000>;
         		pinctrl-names = "cam_default", "cam_suspend";
         		pinctrl-0 = <&cam_sensor_mclk1_default &cam_sensor_front_default &cam_sensor_vio_default &cam_sensor_front_default_stdby &i2c_vt_cam_active>;
         		pinctrl-1 = <&cam_sensor_mclk1_sleep &cam_sensor_front_sleep &cam_sensor_vio_sleep &cam_sensor_front_sleep_stdby &i2c_vt_cam_sleep>;
         		gpios = <&tlmm 27 0>,
             			<&tlmm 129 0>,
             			<&tlmm 134 0>,
             			<&tlmm 97 0>;
         		qcom,gpio-reset = <1>;
         		qcom,gpio-vio = <2>;
         		qcom,gpio-standby = <3>;
         		qcom,gpio-req-tbl-num = <0 1 2 3>;
         		qcom,gpio-req-tbl-flags = <1 0 0 0>;
         		qcom,gpio-req-tbl-label = "CAMIF_MCLK1",
             			"CAM_RESET1",
             			"CAM_VIO_EN",
             			"CAM_STANDBY";
         		qcom,sensor-position = <1>;
         		qcom,sensor-mode = <0>;
         		status = "ok";
         		clocks = <&clock_gcc clk_mclk1_clk_src>,
                 		<&clock_gcc clk_gcc_camss_mclk1_clk>;
         		clock-names = "cam_src_clk", "cam_clk";
             		};

		};
	i2c_6: i2c@7af6000 { /* BLSP2 QUP2 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg = <0x7af6000 0x600>;
		interrupt-names = "qup_irq";
		interrupts = <0 300 0>;
		qcom,clk-freq-out = <400000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp2_ahb_clk>,
			<&clock_gcc clk_gcc_blsp2_qup2_i2c_apps_clk>;
		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_6_active>;
		pinctrl-1 = <&i2c_6_sleep>;
		qcom,noise-rjct-scl = <0>;
		qcom,noise-rjct-sda = <0>;
		qcom,master-id = <84>;
		dmas = <&dma_blsp2 6 64 0x20000020 0x20>,
			<&dma_blsp2 7 32 0x20000020 0x20>;
		dma-names = "tx", "rx";

		audio_codec: rt5659@1b {
			compatible = "realtek,rt5659";
			reg = <0x1b>;
			gpio-controller;
			#gpio-cells = <2>;
			pinctrl-names = "default", "sleep";
			pinctrl-0 = <&rt5659_contorl_active &rt5659_irq_active>;
			pinctrl-1 = <&rt5659_contorl_suspend &rt5659_irq_suspend>;

			interrupts = <35 0>;
			interrupt-parent = <&tlmm>;

			realtek,in3-differential;
			realtek,in4-differential;
			realtek,noise-gate1-hp-enabled;

			realtek,gpio_reset = <&tlmm 71 0x0>;
			realtek,gpio_ldo = <&tlmm 74 0x00>;
			realtek,gpio_3p3_en = <&tlmm 73 0x00>;

			imp_table = <
				/* min      max     gain   bias */
				    0       10       10      5
				    11      21       5       5
				    22      85       0       5
				    86      839      0       1
				    840     1023     0       1
			>;
		};
	};

	det_zones: det_zones {
		#list-det-cells = <4>;
	};

	but_zones: but_zones {
		#list-but-cells = <3>;
	};

	earjack {
		compatible = "sec_jack";
		detect-gpio = <&tlmm 48 0x0>;
		key-gpio = <&tlmm 141 0x0>;
		det-dbtime = <100>;
		key-dbtime = <30>;

		det-zones-list = <&det_zones 0 10000 10 2>,
				 <&det_zones 750 10000 10 2>,
				 <&det_zones 751 10000 10 1>,
				 <&det_zones 9999 10000 10 1>;
		but-zones-list = <&but_zones 226 0 240>, /* KEY_MEDIA:226 */
				 <&but_zones 582 241 278>, /* KEY_VOICECOMMAND:582 */
				 <&but_zones 115 279 365>, /* KEY_VOLUMEUP:115 */
				 <&but_zones 114 366 999>; /* KEY_VOLUMEDOWN:114 */

		pinctrl-names = "earjack_gpio_active",
				"earjack_gpio_suspend";
		pinctrl-0 = <&earjack_det_act &earjack_switch_act>;
		pinctrl-1 = <&earjack_det_sus &earjack_switch_sus>;
	};

	sound {
		compatible = "qcom,msm8952-rt5659-audio-codec";
		qcom,model = "msm8952-rt5659-snd-card";
		reg = <0xc051000 0x4>,
		      <0xc051004 0x4>,
		      <0xc055000 0x4>,
		      <0xc056000 0x4>,
		      <0xc052000 0x4>;
		reg-names = "csr_gp_io_mux_mic_ctl",
			    "csr_gp_io_mux_spkr_ctl",
			    "csr_gp_io_lpaif_pri_pcm_pri_mode_muxsel",
			    "csr_gp_io_lpaif_sec_pcm_sec_mode_muxsel",
			    "csr_gp_io_mux_quin_ctl";

		qcom,codec_irq_n = <&tlmm 35 0x0>;

		qcom,mpp-standalone-mode;
		qcom,mpp-channel-scaling = <4 1 1>;
		qcom,earjack-read-vadc = <&pm8953_vadc>;
		qcom,msm-ext-pa = "quinary";
		qcom,msm-mclk-freq = <19200000>;
		qcom,codec_ear_bias = <0>;
		qcom,msm-hs-micbias-type = "External";
		qcom,audio-routing =
			"HP", "HPOL",
			"HP", "HPOR",
			"RCV", "MONOOUT",
			"SPK", "SPOL",
			"SPK", "SPOR",
			"IN1P", "Headset Mic",
			"IN1N", "Headset Mic",
			"IN3P", "MICBIAS3",
			"IN3P", "Sub Mic",
			"IN3N", "Sub Mic",
			"IN4P", "MICBIAS2",
			"IN4P", "Main Mic",
			"IN4N", "Main Mic";
		qcom,msm-gpios =
			"quat_i2s",
			"quin_i2s";
		qcom,pinctrl-names =
			"all_off",
			"quin_i2s_act",
			"quat_i2s_act",
			"quin_quat_i2s_act";
		pinctrl-names =
			"all_off",
			"quin_i2s_act",
			"quat_i2s_act",
			"quin_quat_i2s_act";
		pinctrl-0 = <&quin_tlmm_lines_sus &quin_tlmm_ws_lines_sus &gp_clk1_suspend>;
		pinctrl-1 = <&quin_tlmm_lines_act &quin_tlmm_ws_lines_act &gp_clk1_active>;
		pinctrl-2 = <&quin_tlmm_lines_act &quin_tlmm_ws_lines_act &gp_clk1_active>;
		pinctrl-3 = <&quin_tlmm_lines_act &quin_tlmm_ws_lines_act &gp_clk1_active>;
		asoc-platform = <&pcm0>, <&pcm1>, <&pcm2>, <&voip>, <&voice>,
				<&loopback>, <&compress>, <&hostless>,
				<&afe>, <&lsm>, <&routing>, <&lpa>;
		asoc-platform-names = "msm-pcm-dsp.0", "msm-pcm-dsp.1",
				"msm-pcm-dsp.2", "msm-voip-dsp",
				"msm-pcm-voice", "msm-pcm-loopback",
				"msm-compress-dsp", "msm-pcm-hostless",
				"msm-pcm-afe", "msm-lsm-client",
				"msm-pcm-routing", "msm-pcm-lpa";
		asoc-cpu = <&dai_pri_auxpcm>,
				<&dai_mi2s0>, <&dai_mi2s1>,
				<&dai_mi2s2>, <&dai_mi2s3>,
				<&dai_mi2s5>, <&dai_mi2s6>,
				<&bt_sco_rx>, <&bt_sco_tx>,
				<&int_fm_rx>, <&int_fm_tx>,
				<&afe_pcm_rx>, <&afe_pcm_tx>,
				<&afe_proxy_rx>, <&afe_proxy_tx>,
				<&incall_record_rx>, <&incall_record_tx>,
				<&incall_music_rx>, <&incall_music_2_rx>;
		asoc-cpu-names = "msm-dai-q6-auxpcm.1",
				"msm-dai-q6-mi2s.0", "msm-dai-q6-mi2s.1",
				"msm-dai-q6-mi2s.2", "msm-dai-q6-mi2s.3",
				"msm-dai-q6-mi2s.5", "msm-dai-q6-mi2s.6",
				"msm-dai-q6-dev.12288", "msm-dai-q6-dev.12289",
